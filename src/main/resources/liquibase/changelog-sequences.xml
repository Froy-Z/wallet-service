<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">

    <changeSet id="1" author="Froy-Z">
        <sql>CREATE SCHEMA IF NOT EXISTS ylab_schema;</sql>
    </changeSet>

    <changeSet id="2" author="Froy-Z">
        <createSequence sequenceName="players_id_seq"
                        schemaName="ylab_schema"
                        cycle="false"
                        minValue="1"
                        maxValue="99999"
                        startValue="1"
                        incrementBy="1"/>
    </changeSet>

    <changeSet id="3" author="Froy-Z">
        <createSequence sequenceName="accounts_id_seq"
                        schemaName="ylab_schema"
                        cycle="false"
                        minValue="1"
                        maxValue="99999"
                        startValue="1"
                        incrementBy="1"/>
    </changeSet>

    <changeSet id="4" author="Froy-Z">
        <createSequence sequenceName="loggingies_id_seq"
                        schemaName="ylab_schema"
                        cycle="false"
                        minValue="1"
                        maxValue="99999"
                        startValue="1"
                        incrementBy="1"/>
    </changeSet>

    <changeSet id="5" author="Froy-Z">
        <createSequence sequenceName="transactions_id_seq"
                        schemaName="ylab_schema"
                        cycle="false"
                        minValue="1"
                        maxValue="99999"
                        startValue="1"
                        incrementBy="1"/>
    </changeSet>


</databaseChangeLog>



























